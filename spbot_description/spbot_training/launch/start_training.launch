<launch>
    <!-- This version uses the openai_ros environments -->
    <rosparam command="load" file="$(find spbot_training)/config/spbot_openai_qlearn_params.yaml" />
    <!-- Launch the training system -->
    <node pkg="spbot_openai_example" name="spbot_world" type="start_qlearning.py" output="screen"/>
</launch>
